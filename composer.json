{
    "name":         "jms/translation-bundle",
    "description":  "Makes translating a site easier while retaining all of the performance optimizations.",
    "keywords":     ["translation","multilanguage"],
    "type":         "symfony-bundle",
    "license":      "Apache",
    "authors": [
        {
            "name":   "Johannes M. Schmitt",
            "email":  "schmittjoh@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": "2.*",
        "nikic/php-parser": "0.9.1",
        "symfony/console": "2.*"
    },
    "require-dev": {
        "symfony/twig-bundle": "*",
        "symfony/browser-kit": "*",
        "symfony/class-loader": "*",
        "symfony/css-selector": "*",
        "symfony/finder": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "symfony/security": "*",
        "symfony/process": "*",
        "symfony/yaml": "*",
        "sensio/framework-extra-bundle": "*",
        "jms/di-extra-bundle": "*"
    },
    "autoload": {
        "psr-0": { "JMS\\TranslationBundle": "" }
    },
    "target-dir": "JMS/TranslationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
